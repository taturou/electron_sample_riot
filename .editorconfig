# EditorConfig is awesome: http://EditorConfig.org

# top-most EditorConfig file
root = true

# every file
[*]
indent_style = space
indent_size = 2
tab_width = 2
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = false
insert_final_newline = false

# ignore
[{/.vscode/**,/node_modules/**,/dist/**,/release/**,/tmp/**}]
indent_style = none
indent_size = none
tab_width = none
end_of_line = none
charset = none
trim_trailing_whitespace = none
insert_final_newline = none